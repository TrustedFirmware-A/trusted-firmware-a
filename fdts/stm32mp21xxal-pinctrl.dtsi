&pinctrl {
	/omit-if-no-ref/
	gpioa: gpio@44240000 {
		status = "okay";
		ngpios = <16>;
		gpio-ranges = <&pinctrl 0 0 16>;
	};

	/omit-if-no-ref/
	gpiob: gpio@44250000 {
		status = "okay";
		ngpios = <13>;
		gpio-ranges = <&pinctrl 1 17 3>, <&pinctrl 5 21 3>, <&pinctrl 9 25 7>;
	};

	/omit-if-no-ref/
	gpioc: gpio@44260000 {
		status = "okay";
		ngpios = <14>;
		gpio-ranges = <&pinctrl 0 32 14>;
	};

	/omit-if-no-ref/
	gpiod: gpio@44270000 {
		status = "okay";
		ngpios = <16>;
		gpio-ranges = <&pinctrl 0 48 16>;
	};

	/omit-if-no-ref/
	gpioe: gpio@44280000 {
		status = "okay";
		ngpios = <16>;
		gpio-ranges = <&pinctrl 0 64 16>;
	};

	/omit-if-no-ref/
	gpiof: gpio@44290000 {
		status = "okay";
		ngpios = <15>;
		gpio-ranges = <&pinctrl 0 80 14>, <&pinctrl 15 95 1>;
	};

	/omit-if-no-ref/
	gpiog: gpio@442a0000 {
		status = "okay";
		ngpios = <15>;
