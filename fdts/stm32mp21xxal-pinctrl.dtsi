	/omit-if-no-ref/
	gpioi: gpio@442c0000 {
		status = "okay";
		ngpios = <6>;
		gpio-ranges = <&pinctrl 0 128 2>, <&pinctrl 4 132 3>, <&pinctrl 8 136 1>;
	};
};

&pinctrl_z {
